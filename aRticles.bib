 @misc{leuc, title={Leucanthemum vulgare}, url={https://en.wikipedia.org/w/index.php?title=Leucanthemum_vulgare&oldid=1077100813}, abstractNote={Leucanthemum vulgare, commonly known as the ox-eye daisy, oxeye daisy, dog daisy, marguerite (French: Marguerite commune, “common marguerite”) and other common names, is a widespread flowering plant native to Europe and the temperate regions of Asia, and an introduced plant to North America, Australia and New Zealand.}, note={Page Version ID: 1077100813}, journal={Wikipedia}, author={Wikipedia}, year={2022}, month={Mar}, language={en} }
 
