 @misc{bap, type={Database}, title={Baptisia australis (Blue False Indigo, Blue Wild Indigo, False Indigo, Wild Indigo)}, url={https://plants.ces.ncsu.edu/plants/baptisia-australis/}, journal={North Carolina Extension Gardener Plant Toolbox}, author={NCSU} }
 
  @misc{bap2, title={Baptisia australis (False Indigo)}, url={https://www.gardenia.net/plant/baptisia-australis-false-indigo}, abstractNote={Tough and durable, Baptisia australis (False Indigo) is an upright perennial with a long season of interest. In spring, this native to the North American prairies bears spikes of pea-shaped indigo blue flowers, resembling Lupines. They last for about three weeks and attract butterflies, bees, and hummingbirds. When the flowers fade away, the lovely blue-green, trifoliate leaves remain neat and form a lovely backdrop for the other perennials in the garden. If left untrimmed, the plant forms interesting seedpods turning deep black in the fall and persist into winter.}, journal={Gardenia.net}, author={Gardenia} }

