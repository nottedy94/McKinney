   @misc{multi2, title={Rosa multiflora}, url={https://en.wikipedia.org/w/index.php?title=Rosa_multiflora&oldid=1088183196}, abstractNote={Rosa multiflora (syn. Rosa polyantha) is a species of rose known commonly as multiflora rose, baby rose, Japanese rose, many-flowered rose, seven-sisters rose, Eijitsu rose and rambler rose. It is native to eastern Asia, in China, Japan and Korea. It should not be confused with Rosa rugosa, which is also known as “Japanese rose”, or with polyantha roses which are garden cultivars derived from hybrids of R. multiflora.  It was introduced to North America, where it is regarded as an invasive species.}, note={Page Version ID: 1088183196}, journal={Wikipedia}, author={Wikipedia}, year={2022}, month={May}, language={en} }
   
